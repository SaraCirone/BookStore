package it.saracirone.fabula.config;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;
import org.springframework.context.annotation.Scope;

import it.saracirone.fabula.entities.Ruolo;
import it.saracirone.fabula.entities.TipoRuolo;
import it.saracirone.fabula.entites.Utente;

@Configuration; 

public class Beans {
	
	@Bean
	@Scope("prototype")
	public Utente utente(String nome, String cognome, String username, String password, String email) {
		.nome(nome)
		.cognome(cognome)
		.username(username) 
		.password(password)
		.email(email)
		.attivo(true)
		.build();
		
	}

	
	@Bean
	@Scope("prototype")
	public Utente utente(String nome, String cognome, String username, String password, String email) {
		.nome(nome)
		.cognome(cognome)
		.username(username) 
		.password(password)
		.email(email)
		.attivo(true)
		.build();
		
	}

}
